{
    "name": "topsystem/topadmin",
    "description": "Based on the background management system developed by laravel to help developers quickly build websites",
    "autoload": {
        "psr-4": {
            "TopSystem\\TopAdmin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Admin",
            "email": "admin@topsystem.cc"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/support": "~6.0|~7.0|~8.0|~9.0|~10.0",
        "illuminate/routing": "~6.0|~7.0|~8.0|~9.0|~10.0",
        "laravel/ui": ">=1.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.5|^3.1",
        "larapack/doctrine-support": "~0.1.4",
        "arrilot/laravel-widgets": "^3.13",
        "intervention/image": "^2.7",
        "league/flysystem": "~1.1|~2.0|~3.0",
        "prettus/l5-repository": "^2.8"
    },
    "require-dev": {
        "phpunit/phpcov": ">=6.0",
        "phpunit/phpunit": ">=8.0",
        "laravel/framework": "~6.0|~7.0|~8.0|~9.0",
        "orchestra/testbench": ">=4.0",
        "laravel/browser-kit-testing": ">=5.0.0",
        "orchestra/testbench-browser-kit": ">=4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "TopSystem\\TopAdmin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TopSystem\\TopAdmin\\AdminServiceProvider"
            ]
        }
    }
}
